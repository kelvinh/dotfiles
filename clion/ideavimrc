let mapleader = ','

nnoremap <C-j> <C-w>j
nnoremap <C-k> <C-w>k
nnoremap <C-h> <C-w>h
nnoremap <C-l> <C-w>l

noremap <Leader>h :split<CR>
noremap <Leader>v :vsplit<CR>

nnoremap <Leader>w :w<CR>
nnoremap <Leader><Space> :noh<CR>

nnoremap j gj
nnoremap k gk

vnoremap < <gv
vnoremap > >gv

" vnoremap <Leader>zf :action CollapseSelection<CR>
" this is really really tricky!!
vnoremap zf :<bs><bs><bs><bs><bs>action VimVisualSwapSelections<CR>:action CollapseSelection<CR>

" Move visual block
vnoremap J :m '>+1<CR>gv=gv
vnoremap K :m '<-2<CR>gv=gv

" replace selected content and do NOT save replaced content in register
vnoremap <Leader>P "_dP

" copy/paste from/to system clipboard
vnoremap <Leader>y  "+y
vnoremap <Leader>p  "+p
nnoremap <Leader>p  "+p
nnoremap <Leader>yy "+yy
nnoremap <Leader>yw "+yiw
nnoremap <Leader>y' "+yi'
nnoremap <Leader>y" "+yi"

nnoremap <A-,> :action Back<CR>
nnoremap <A-/> :action Forward<CR>
nnoremap <A-.> :action GotoDeclaration<CR>
nnoremap <A-o> :action GotoRelated<CR>

nnoremap <A-h> :action PreviousTab<CR>
nnoremap <A-l> :action NextTab<CR>

nnoremap <A-m> :action FileStructurePopup<CR>

nnoremap <Leader>eo :action OpenEditorInOppositeTabGroup<CR>
nnoremap <Leader>em :action MoveEditorToOppositeTabGroup<CR>

" nnoremap <Leader>ff :action SearchEverywhere<CR>
nnoremap <Leader>ff :action GotoFile<CR>
nnoremap <Leader>fc :action GotoClass<CR>
nnoremap <Leader>fb :action Switcher<CR>
nnoremap <Leader>fd :action CloseContent<CR>

nnoremap <Leader>rj :action GotoDeclaration<CR>
nnoremap <Leader>rf :action FindUsages<CR>
nnoremap <Leader>rb :action Back<CR>
nnoremap <Leader>rc :action CMake.ReloadCMakeProject<CR>

nnoremap <Leader>mk :action Build<CR>

nnoremap <CR> :

set easymotion
set surround
set multiple-cursors


